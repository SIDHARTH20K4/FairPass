# WalletConnect Configuration (Required)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Sonic Testnet (enabled by default)
NEXT_PUBLIC_ENABLE_TESTNETS=true

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:4000/api

# Sonic Testnet Configuration


# Contract Addresses (Need to be deployed to Sonic testnet)
NEXT_PUBLIC_EVENT_FACTORY_ADDRESS=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_PLATFORM_OWNER_ADDRESS=your_platform_owner_address_here

# Optional: Sonic API Key for contract verification
NEXT_PUBLIC_SONIC_API_KEY=

# Network Details for MetaMask:
# Network Name: Sonic Testnet
# RPC URL: https://rpc.testnet.soniclabs.com
# Explorer URL: https://testnet.sonicscan.org
# Chain ID: 14601
# Currency symbol: S
# Faucet: https://testnet.soniclabs.com/account
